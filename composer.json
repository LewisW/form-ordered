{
    "name": "tenolo/form-ordered",
    "description": "Provides a form ordering support with the Symfony form component.",
    "keywords": [ "symfony", "form", "order" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de"
        },
        {
            "name": "Dennis Hebbeler",
            "email": "dh@tenolo.de"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/form": "^2.8|^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^2.8|^3.4|^4.0"
    },
    "autoload": {
        "psr-4": { "Tenolo\\FormOrdered\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tenolo\\Tests\\FormOrdered\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}

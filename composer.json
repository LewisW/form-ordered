{
    "name": "tenolo/form-ordered",
    "description": "Provides a form ordering support with the Symfony form component.",
    "keywords": [
        "symfony",
        "form",
        "order"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de"
        },
        {
            "name": "Dennis Hebbeler",
            "email": "dh@tenolo.de"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/form": "^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\FormOrdered\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Tests\\FormOrdered\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "scripts": {
        "cs": "phpcs --standard=PSR2 -n src",
        "cbf": "phpcbf --standard=PSR2 -n src",
        "unit": "phpunit --colors=always",
        "test": [
            "@unit",
            "@cs"
        ]
    }
}
